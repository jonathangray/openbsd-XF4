#include <Server.tmpl>

SRCS =	macppcInit.c \
	macppcFbs.c \
	macppcIo.c \
	macppcKbd.c \
	macppcMouse.c \
	macppcfb.c \
	macppcKeyMap.c

OBJS =	macppcInit.o \
	macppcFbs.o \
	macppcIo.o \
	macppcKbd.o \
	macppcMouse.o \
	macppcfb.o \
	macppcKeyMap.o

INCLUDES = -I. -I$(SERVERSRC)/mfb -I$(SERVERSRC)/mi -I$(SERVERSRC)/cfb \
	   -I$(SERVERSRC)/include -I$(SERVERSRC)/os \
	   -I$(XINCLUDESRC) -I$(EXTINCSRC)


NormalLibraryObjectRule()

all::


NormalLibraryTarget(macppc,$(OBJS))

InstallManPage(Xmacppc,$(MANDIR))

DependTarget()
