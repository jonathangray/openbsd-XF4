#define IHaveSubdirs
#define PassCDebugFlags

#if BuildXvLibrary
XVDIR = Xv
#endif
#if BuildGLXLibrary
GLXDIRS = GL GLw
#endif
#if BuildGLULibrary
GLUDIRS = GLU
#endif
#if BuildDmxLibrary
DMXDIR = DMX
#endif

#if BuildXprintLib
XPRINTLIBDIRS = Xp
#endif

#if InstallMiscManPages
MISCDIR = misc
#endif

#if InstallLibManPages
LIBMANDIRS = X11 Xt Xext Xi Xau $(XVDIR) $(GLXDIRS) $(GLUDIRS) $(DMXDIR) $(XPRINTLIBDIRS)
#endif

SUBDIRS = general $(LIBMANDIRS) $(MISCDIR)

all::

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
