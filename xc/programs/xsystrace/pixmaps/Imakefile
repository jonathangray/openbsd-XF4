#define PixmapInstall InstallNonExecFileNoClobber

#define PixmapDir	Concat(IncRoot,/pixmaps)

PixmapInstall(deny-always.xpm,PixmapDir)
PixmapInstall(deny.xpm,PixmapDir)
PixmapInstall(logo.xpm,PixmapDir)
PixmapInstall(permit-always.xpm,PixmapDir)
PixmapInstall(permit.xpm,PixmapDir)
PixmapInstall(skull.xpm,PixmapDir)
