#define IHaveSubdirs

      TESTDATA = 
     DATAFILES = am az be bg br by ca ca_enhanced cz cz_qwerty ctrl \
		 de de_CH dk dvorak ee el en_US es fi fr fr_CH \
		 gb group hu hu_US hr il il_phonetic iso9995-3 inet is it jp \
		 keypad lock lt lt_std lt_p lv mk no pc104 \
		 pl pt \
		 ralt ro ru se si sk sk_qwerty sr th ua us us_intl \
		 us_group3 vn tr
       SUBDIRS = digital fujitsu hp macintosh nec sgi sony sun xfree68

MakeXkbDir($(LIBDIR)/xkb,symbols)
InstallMultiple($(DATAFILES),$(LIBDIR)/xkb/symbols)
DependSubdirs($(SUBDIRS))
