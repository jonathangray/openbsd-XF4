OBJS2 = icroi.o iroi.o
#endif

       SRCS = $(SRCS1) $(SRCS2)

       OBJS = $(OBJS1) $(OBJS2)

   INCLUDES = -I../../include -I$(SERVERSRC)/include -I$(XINCLUDESRC) -I$(EXTINCSRC)
   LINTLIBS = ../export/llib-lexp.ln ../import/llib-limp.ln \
              ../process/llib-lproc.ln ../request/llib-lreq.ln

