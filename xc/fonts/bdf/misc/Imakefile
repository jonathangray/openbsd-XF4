/*
 * Default to installing "truncated" versions of the UCS fonts.  This means
 * that glyphs above 0x3200 are marked "unencoded" to keep XFontStruct small.
 */

#ifndef TruncateUCSFonts
#ifdef MakeTruncatedUCSBdfFont
#define TruncateUCSFonts YES
#else
#define TruncateUCSFonts NO
#endif
#endif

#ifndef MaxUCSChar
#define MaxUCSChar 0x3200
#endif

/*
 * Default to building 8-bit subsets derived from the UCS master fonts.
 * Currently this means building all the ISO8859 and KOI8-R subsets that
 * are supported.
 */

#ifndef DerivedUCSFonts
#ifdef MakeBdfFontFromUCSMaster
#define DerivedUCSFonts YES
#else
#define DerivedUCSFonts NO
#endif
#endif

