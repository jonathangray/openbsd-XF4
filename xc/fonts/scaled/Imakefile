#define IHaveSubdirs
#define PassCDebugFlags /* as nothing */

#if BuildSpeedoFonts
SPEEDODIR = Speedo
#endif
#if BuildType1Fonts
TYPE1DIR = Type1
#endif
#if BuildCIDFonts
CIDDIR = CID
#endif

        SUBDIRS = $(SPEEDODIR) $(TYPE1DIR) $(CIDDIR)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
